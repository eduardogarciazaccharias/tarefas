# questao10
#include <stdio.h>
#include <stdlib.h>

int main()
{
    char vet[4];
    double vet[4];
    int i;
    for(i=0;i<4;i++){              **primeiro trecho para char - 1 byte para cada valor
        printf("%X \n\n",vet+i);
    }
    return 0;
}

#include <stdio.h>
#include <stdlib.h>

int main()
{
    int vet[4];
    int i;
    for(i=0;i<4;i++){              **segundo trecho para int- 4 bytes para cada valor;
        printf("%X \n\n",vet+i);
    }
    return 0;
}

#include <stdio.h>
#include <stdlib.h>

int main()
{
    float vet[4];
    int i;
    for(i=0;i<4;i++){             **terceiro trecho para float - 4 bytes para cada valor
        printf("%X \n\n",vet+i);
    }
    return 0;
}

#include <stdio.h>
#include <stdlib.h>

int main()
{
    double vet[4];
    int i;
    for(i=0;i<4;i++){               **quarto trecho para double - 8 bytes para cada valor
        printf("%X \n\n",vet+i);
    }
    return 0;
}
